{
    "name": "reactcodesnippets",
    "displayName": "React Code Snippets",
    "description": "Simple snippets for React Js frontend developers",
    "version": "1.0.0",
    "publisher": "FernandaKipper",
    "repository": {
        "type": "git",
        "url": "https://github.com/Fernanda-Kipper/VSCodeExtension-ReactSnippets.git"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascriptreact",
                "path": "./snippets/functionComponentsJS_snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/functionComponentsJS_snippets.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/functionComponentsTS_snippets.code-snippets"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/functionComponentsTS_snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/hooksJS_snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/hooksJS_snippets.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/hooksTS_snippets.code-snippets"
            },
            {
                "language": "typescriptreact",
                "path": "./snippets/hooksTS_snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/unitTest_snippets.code-snippets"
            },
            {
                "language": "typescript",
                "path": "./snippets/unitTest_snippets.code-snippets"
            }
        ]
    }
}
